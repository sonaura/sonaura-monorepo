{
  "name": "sonaura-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "cf-typegen": "turbo cf-typegen",
    "deploy": "turbo deploy"
  },
  "devDependencies": {
    "@sonaura/eslint-config": "workspace:*",
    "@sonaura/typescript-config": "workspace:*",
    "prettier": "3.5.3",
    "turbo": "2.5.2",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": "22"
  }
}
